{
  "expo": {
    "owner": "tleyden",
    "name": "vibemachine",
    "slug": "vibemachine",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "vibemachine",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tleyden.vibemachine",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSMicrophoneUsageDescription": "Vibemachine needs microphone access to stream audio to OpenAI.",
        "UIBackgroundModes": [
          "audio"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.tleyden.vibemachine"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "assetBundlePatterns": [
      "assets/sounds/*"
    ],
    "plugins": [
      [
        "expo-asset",
        {
          "assets": [
            "./assets/sounds/artybeeps1.mp3",
            "./assets/sounds/artybeeps2.mp3",
            "./assets/sounds/artybeeps3.mp3",
            "./assets/sounds/artybeeps4.mp3",
            "./assets/sounds/artybeeps5.mp3"
          ]
        }
      ],
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-secure-store",
      "expo-web-browser",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "f3a6322c-ba58-4e74-902e-89a4d9fccb52"
      }
    },
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/f3a6322c-ba58-4e74-902e-89a4d9fccb52"
    }
  }
}
